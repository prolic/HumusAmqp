listeners.tcp.default = 5672
listeners.ssl.default = 5671
ssl_options.cacertfile           = /rabbitmq-certs/cacert.pem
ssl_options.certfile             = /rabbitmq-certs/cert.pem
ssl_options.keyfile              = /rabbitmq-certs/key.pem
ssl_options.verify               = verify_none
ssl_options.fail_if_no_peer_cert = false
